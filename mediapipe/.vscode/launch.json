    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch multi_hand_tracking_gpu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bazel-bin/mediapipe/examples/desktop/multi_hand_tracking/multi_hand_tracking_gpu",
            "args": [
                "--calculator_graph_config_file=mediapipe/graphs/hand_tracking/multi_hand_tracking_mobile.pbtxt",
                "--input_video_path=/home/datagroup/Videos/SL/input",
                "--output_video_path=/home/datagroup/Videos/SL/output"
                
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "bazel build multi_hand_tracking_gpu"
        } 
        
    ]
}